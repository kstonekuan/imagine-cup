{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["*.{css,scss,js,png,gif,ico,jpg,svg}"]
  },
  "routes": [
    {
      "route": "/.auth/login/aad",
      "statusCode": 404
    },
    {
      "route": "/api/profile/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/profile/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/api/mentors/*",
      "allowedRoles": ["authenticated"]
    }
  ]
}
